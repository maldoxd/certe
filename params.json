  "body": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n\r\n<head>\r\n\r\n    <meta charset=\"utf-8\">\r\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\r\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">\r\n    <meta name=\"description\" content=\"\">\r\n    <meta name=\"author\" content=\"\">\r\n\r\n    <title>Certe Sandwichs</title>\r\n\r\n    <!-- Bootstrap Core CSS -->\r\n    <link href=\"css/bootstrap.min.css\" rel=\"stylesheet\">\r\n    <link href=\"css/handsometable.css\" media=\"all\" rel=\"stylesheet\" type=\"text/css\" />\r\n\r\n    <link rel=\"stylesheet\" href=\"css/kendo.common.min.css\" />\r\n    <link rel=\"stylesheet\" href=\"css/kendo.default.min.css\" />\r\n    <link rel=\"stylesheet\" href=\"css/kendo.default.mobile.min.css\" />\r\n    <script src=\"js/jquery.min.js\"></script>\r\n    <script src=\"js/handsometable.js\"></script>\r\n    <script src=\"js/kendo.all.min.js\"></script>\r\n</head>\r\n\r\n<!-- Custom CSS -->\r\n<style>\r\n    body {\r\n        padding-top: 70px;\r\n        /* Required padding for .navbar-fixed-top. Remove if using .navbar-static-top. Change if height of navigation changes. */\r\n    }\r\n</style>\r\n\r\n\r\n</head>\r\n\r\n<body>\r\n\r\n\r\n\r\n    <!-- Page Content -->\r\n    <div class=\"container\">\r\n\r\n        <div class=\"row\">\r\n            <div class=\"col-lg-12 text-center\">\r\n                <h1>Sandwich Template</h1>\r\n                <p class=\"lead\">Selecciona dia</p>\r\n                <select id=\"DaySelect\">\r\n  <option value=\"Monday\">Monday</option>\r\n  <option value=\"Tuesday\" selected=\"\">Tuesday</option> \r\n  <option value=\"Wednesday\">Wednesday</option> \r\n  <option value=\"Thursday\">Thursday</option> \r\n  <option value=\"Friday\">Friday</option>  \r\n</select>\r\n                <input id=\"Check\" type=\"button\" value=\"CHECK\">\r\n                <div id=\"example\"></div>\r\n\r\n                <div id=\"grid2\"></div>\r\n            </div>\r\n        </div>\r\n        <!-- /.row -->\r\n        <input id=\"print\" type=\"button\" hidden=\"\" value=\"CHECK\">\r\n    </div>\r\n    <!-- /.container -->\r\n    <div id=\"grid\" class=\"col-md-12 row\"></div>\r\n    <!-- Bootstrap Core JavaScript -->\r\n    <script src=\"js/bootstrap.min.js\"></script>\r\n\r\n</body>\r\n\r\n</html>\r\n<script src=\"https://npmcdn.com/dropbox/dist/Dropbox-sdk.min.js\"></script>\r\n<script src=\"js/global.js\"></script>\r\n<script>\r\n    var htContents;\r\n    var container = document.getElementById('example');\r\n    $(\"#grid2\").kendoGrid({\r\n        dataSource: {\r\n            batch: true,\r\n            schema: {\r\n                model: {\r\n                    fields: {\r\n                        qty: {},\r\n                        stage: {},\r\n                    }\r\n                }\r\n            },\r\n        },\r\n        toolbar: [\"create\"],\r\n        columns: [\r\n\r\n            {\r\n                field: \"Cantidad\",\r\n                title: \"Cantidad\",\r\n                width: \"120px\"\r\n            }, {\r\n                field: \"Stage\",\r\n                title: \"Stage\",\r\n                width: \"120px\"\r\n            }\r\n        ],\r\n        editable: true\r\n    });\r\n\r\n    var data = new Array();\r\n\r\n    var arrayName = new Array();\r\n    $(\"#Check\").click(function() {\r\n        var list = $(\"#grid\");\r\n        list.empty();\r\n        var day = $(\"#DaySelect\").val();\r\n        var daySelection = Monday;\r\n        var dayDist = MondayDist;\r\n        switch (day) {\r\n            case 'Tuesday':\r\n                daySelection = Tuesday;\r\n                dayDist = TuesdayDist;\r\n                break;\r\n            case 'Wednesday':\r\n                daySelection = Wednesday;\r\n                dayDist = WednesdayDist;\r\n                break;\r\n            case 'Thursday':\r\n                daySelection = Thursday;\r\n                dayDist = ThursdayDist;\r\n                break;\r\n            case 'Friday':\r\n                daySelection = Friday;\r\n                dayDist = FridayDist;\r\n        }\r\n        addDay(daySelection, JSON.parse(JSON.stringify(dayDist)), day);\r\n\r\n    });\r\n\r\n    function addDay(daySand, Daydist, day) {\r\n        var grid = $(\"#grid2\").data(\"kendoGrid\");\r\n        var _arrayData = grid.dataSource.data();\r\n\r\n        for (var i = 0; i < _arrayData.length; i++) {\r\n            var qty = _arrayData[i].Cantidad;\r\n            var DaySand = JSON.parse(JSON.stringify(daySand));\r\n            var data = new Array();\r\n            if (qty != null) {\r\n                list = $(\"#grid\");\r\n                list.append('<div class=\"col-xs-6\"></div>')\r\n                list = list.find('div').last();\r\n                var text = _arrayData[i].Stage!=null?_arrayData[i].Stage: \"\";\r\n                list.append('<ul><b>' + qty + 'pp ' + day + ': ' + text + '</b></ul>');\r\n                list = list.find('ul').last();\r\n                var total = 0;\r\n                if (qty < 7) {\r\n                    DaySand[0].splice(Math.floor((Math.random() * 2) - 1), 1);\r\n                    DaySand[2].splice(Math.floor((Math.random() * 2) - 1), 1);\r\n                    DaySand[3].splice(Math.floor((Math.random() * 2) - 1), 1);\r\n                }\r\n                for (var j = 0; j < DaySand.length; j++) {\r\n                    var dist = 0;\r\n                    for (var k = 0; k < DaySand[j].length; k++) {\r\n                        var selected = true;\r\n                        if (qty <= 5) {\r\n                            dist = Math.round((qty * Daydist[j] / DaySand[j].length / 100));\r\n                        }     else {\r\n                            if (j == DaySand.length -1) {\r\n                              if (k==0)\r\n                                dist = Math.round((qty - total) / 2);\r\n                              else\r\n                                dist=qty-total;\r\n                            } \r\n                            else {\r\n                                if (selected){\r\n\r\n                                var ran = Math.floor((Math.random() * 2) - 1);\r\n                                if (ran==0)\r\n                                   dist = Math.ceil((qty * Daydist[j] / DaySand[j].length / 100)) ;\r\n                                else\r\n                                {\r\n                                    dist = Math.floor((qty * Daydist[j] / DaySand[j].length / 100)) ;\r\n                                    selected=false;\r\n                                }\r\n                                }\r\n                                else\r\n                                   dist = Math.ceil((qty * Daydist[j] / DaySand[j].length / 100)) ;\r\n                            }\r\n                        }\r\n                        total += dist;\r\n                        data.push([DaySand[j][k],dist]);\r\n\r\n                    }\r\n\r\n                }\r\n                \r\n                for (var l = 0; l < data.length; l++) {\r\n                    list.append('<ol>' + data[l][0] + ': ' + data[l][1] + '</ol>');\r\n\r\n                }\r\n            }\r\n\r\n            $(\"#print\").show();\r\n        }\r\n\r\n    }\r\n    $(\"#print\").click(function() {\r\n\r\n        $(\".container\").hide();\r\n        $(\"#grid\").show;\r\n        window.print();\r\n        $(\".container\").show();\r\n    });\r\n</script>",
